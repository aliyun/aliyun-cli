{
  "name": "AddLivePullStreamInfoConfig",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "descriptions": {
    "zh": "添加直播拉流信息，仅支持拉取直播流。\n"
  },
  "parameters": [
    {
      "name": "AccessKeyId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "当认证方式为Accesskey时必填，用户需要显式指定，校验签名使用，不指定则不校验签名"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "Action",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "系统规定参数，取值：AddLivePullStreamInfoConfig"
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "SecurityToken",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "OwnerId",
      "position": "Query",
      "type": "Long",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "DomainName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "您的拉流域名为用户的播放域名"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "AppName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "直播流所属应用名称"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "StreamName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "直播流名"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "SourceUrl",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "用户的直播流所在的源站，多个源站可以填多个地址，用分号分隔"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "StartTime",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "拉流开始时间，UTC格式, StartTime和EndTime时间间隔在7天内"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "EndTime",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "拉流结束时间，UTC格式, StartTime和EndTime时间间隔在7天内,且EndTime超过当前时间"
      },
      "required": true,
      "hidden": false
    }
  ]
}